{
  "name": "dc-create-web-app",
  "version": "0.1.0",
  "description": "多引擎可插拔的前端脚手架",
  "type": "module",
  "bin": {
    "create-web-app": "dist/index.js"
  },
  "files": [
    "dist",
    "src/templates"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "dev": "node src/index.js",
    "build": "tsup && node scripts/build.mjs",
    "prepare": "pnpm build",
    "release": "pnpm build && npm publish --access public",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@babel/generator": "^7.26.3",
    "@babel/parser": "^7.26.3",
    "@babel/traverse": "^7.25.9",
    "@babel/types": "^7.26.3",
    "@clack/prompts": "^0.7.0",
    "commander": "^12.0.0",
    "execa": "^8.0.1",
    "fs-extra": "^11.2.0",
    "picocolors": "^1.0.0"
  },
  "devDependencies": {
    "tsup": "^8.3.0",
    "typescript": "^5.6.3",
    "vitepress": "^1.3.0"
  }
}
